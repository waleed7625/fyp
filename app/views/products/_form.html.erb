<div class='row'>
<div class='col-xs-12'>
  <%= form_with(url: user_products_path(current_user), :html =>{class:"form-horizontal",role: "form"}) do |f|%>
  <div class="form-group">
  <div class="control-label col-sm-2">
  <%=f.label :name%>
  </div>
  <div class="col-sm-8">
  <%=f.text_field :name, class: "form-control", placeholder:"Name for your product", autofocus: true%>
  </div>
</div>


  
  <div class="form-group">
  <div class="control-label col-sm-2">
  <%= f.label :Description%>
  </div>
    <div class="col-sm-8">
  <%= f.text_area :description, class:"form-control",rows:10,placeholder: "Description of product"%>
  </div>
</div>

  <div class="form-group">
  <div class="control-label col-sm-2">
  <%=f.label :Price%>
  </div>
  <div class="col-sm-8">
  <%=f.text_field :price, class: "form-control", placeholder:"Price of your Product", autofocus: true%>
  </div>
    <div class="upload-btn">
    <%= f.label :images %>
    <%= f.file_field :images, multiple: true %>
  </div>

</div>
 <div class="form-group">
 <div class="col-sm-offset-2 col-sm-10">
  <%=f.submit class: 'btn btn-primary btn-lg'%>
  </div>
  </div>
  <% end %>
  <div class="col-xs-4 col-xs-offset-4">

</div>
</div>
</div>